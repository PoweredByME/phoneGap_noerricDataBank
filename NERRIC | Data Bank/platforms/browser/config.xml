<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.nust_messlife" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>NOERRIC | Data Bank</name>
    <description>
        This is a simple that gets image from your camera and 
        upload it automatically to NOERRIC Data center. The purpose
        of this app is to make a dataset for their Machine learning 
        Algorithm
    </description>
    <author email="saadkwi12@hotmail.com" href="http://phonegap.com">
        Saad Ahmad
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
